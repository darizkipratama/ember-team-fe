<div class="flex flex-wrap">
    {{#each @lists as |member|}}
        <div class="w-1/3 p-4">
            <div class="bg-white rounded-lg shadow-lg p-4">
                <h2 class="text-xl font-bold text-blue-700">{{member.memberName}}</h2>
                <p class="text-gray-700">{{member.memberRole}}</p>
            </div>
        </div>
    {{/each}}
</div>